{
  "name": "school-management-v1-api",
  "version": "1.0.0",
  "description": "A comprehensive API for managing school-related data, including sessions, terms, calensers, classes, school fess etc. Built with Express and PosgreSQL, the API serves as the backend for a school management system",
  "main": "app.js",
  "scripts": {
    "start": "nodemon src/app.js",
    "migrate": "npx sequelize-cli db:migrate",
    "down": "npx sequelize-cli db:migrate:undo"
  },
  "author": "HumSad",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "moment": "^2.30.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2",
    "xss": "^1.0.14",
    "xss-clean": "^0.1.4",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.3",
    "sequelize-cli": "^6.6.2"
  }
}
